{
    "name": "Parse Me",
    "description": "Parse any page!!",
    "version": "1.0",
    // "icons": {
    //     "16": "icon16.png",
    //     "48": "icon48.png",
    //     "128": "icon128.png"
    // },
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.min.js",                
                "content.js"
            ]
        }
    ],
    "manifest_version": 2
}
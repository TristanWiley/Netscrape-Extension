{
    "manifest_version": 2,

    "name": "Parse Me",
    "description": "Parse any page!!",
    "version": "1.0",

    "browser_action": {
        // "default_icon": "icon.png",
        "default_popup": "popup.html"
      },

    "permissions": [
        "activeTab",
        "storage"
    ],

    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
